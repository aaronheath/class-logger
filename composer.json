{
  "name": "aaronheath/class-logger",
  "description": "Simple logging helper trait for use within Laravel classes.",
  "license": "MIT",
  "keywords": ["laravel", "logging", "logs"],
  "authors": [
    {
      "name": "Aaron Heath",
      "email": "aaron@aaronheath.com",
      "homepage": "https://aaronheath.com"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "illuminate/support": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Heath\\ClassLogger\\": "src/"
    }
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "^9.0",
    "orchestra/testbench": "^6.0",
    "sempro/phpunit-pretty-print": "^1.0",
    "timacdonald/log-fake": "^1.6",
    "spatie/laravel-ray": "^1.17"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable"
}
